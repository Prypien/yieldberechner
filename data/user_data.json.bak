  "vm_yield_models": [
    {
      "id": "YM_POI",
      "name": "Poisson (VM)",
      "params": {
        "alpha": 3,
        "n": 1
      },
      "formula": "VM = exp(-D * A)",
      "uses_params": [],
      "description": "Poisson-Modell. Verwendet keine zusätzlichen Parameter."
    },
    {
      "id": "YM_MUR",
      "name": "Murphy (VM)",
      "params": {
        "alpha": 3,
        "n": 1
      },
      "formula": "VM = (1 - exp(-D * A)) / (D * A)",
      "uses_params": [],
      "description": "Murphy-Modell. Glättet Poisson bei kleinen Flächen."
    },
    {
      "id": "YM_SEEDS",
      "name": "Seeds (VM)",
      "params": {
        "alpha": 3,
        "n": 1
      },
      "formula": "VM = exp(-sqrt(D * A))",
      "uses_params": [],
      "description": "Seeds-Modell. Schwächere Flächenabhängigkeit."
    },
    {
      "id": "YM_BOSE",
      "name": "Bose–Einstein (VM)",
      "params": {
        "alpha": 3,
        "n": 2
      },
      "formula": "VM = (1 / (1 + D * A)) ^ n",
      "uses_params": [
        "n"
      ],
      "description": "Bose–Einstein-Modell. Exponent n steuert Cluster-Effekt."
    },
    {
      "id": "YM_NEGBIN",
      "name": "Negative Binomial (VM)",
      "params": {
        "alpha": 3,
        "n": 1
      },
      "formula": "VM = (1 + (D * A) / alpha) ^ (-alpha)",
      "uses_params": [
        "alpha"
      ],
      "description": "Negative-Binomial-Modell. alpha steuert Defekt-Clusterung."
    }
  ],
